# Comments at the beginning of the file
# This is a comment for documentation

# Simple variables
DATABASE_NAME=myapp_db
PORT=3000
DEBUG=true

# Variables with spaces (should be ignored around the = sign)
SPACED_VAR = value_with_spaces
  INDENTED_VAR=indented_value

# Empty lines should be ignored


# Variables in quotes
SINGLE_QUOTED='single quoted value'
DOUBLE_QUOTED="double quoted value"
MIXED_QUOTES='value with "inner" quotes'

# Variables with escaping
ESCAPED_QUOTES="value with \"escaped\" quotes"
ESCAPED_NEWLINE="line1\nline2"
ESCAPED_TAB="value\twith\ttabs"

# Multiline values
MULTILINE_SINGLE='line1
line2
line3'
MULTILINE_DOUBLE="line1
line2
line3"

# Variables with special characters
SPECIAL_CHARS="!@#$%^&*()_+-=[]{}|;:,.<>?"
URL_VALUE="https://example.com:8080/path?param=value&other=123"
EMAIL_VALUE=user@example.com

# Variables with empty values
EMPTY_VALUE=
EMPTY_QUOTED=""
EMPTY_SINGLE=''

# Variables with whitespace only
WHITESPACE_ONLY="   "
TAB_ONLY="		"

# Variables with interpolation (depends on the parser)
HOME_DIR=/home/user
LOG_FILE=${HOME_DIR}/logs/app.log
BACKUP_PATH=$HOME_DIR/backups

# Boolean values
IS_PRODUCTION=false
ENABLE_CACHE=TRUE
SHOW_DEBUG=0
SEND_EMAILS=1

# Numeric values
MAX_CONNECTIONS=100
TIMEOUT_SECONDS=30.5
NEGATIVE_NUMBER=-42

# Variables with underscores and numbers
API_KEY_V2=abc123def456
USER_ID_123=user_value
_PRIVATE_VAR=private_value

# Long values
LONG_TEXT="Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris."

# Variables with hashes and tokens
JWT_SECRET=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9
API_TOKEN=sk-1234567890abcdefghijklmnopqrstuvwxyz
HASH_VALUE=a1b2c3d4e5f6789012345678901234567890abcd

# Comments after values (not supported by some parsers)
INLINE_COMMENT=value # this is a comment
INLINE_COMMENT_SINGLE_QUOTED='value' # this is a comment
INLINE_COMMENT_DOUBLE_QUOTED="value" # this is a comment
ANOTHER_VALUE=test#notcomment

# Variables with exotic names (may not be supported by all parsers)
CAMELCASE=camelValue
kebab-case=kebabValue
dot.notation=dotValue

# Values containing equals sign
EQUATION="x=y+z"
BASE64_VALUE="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR42mNk+M9QDwADhgGAWjR9awAAAABJRU5ErkJggg=="

# Variables with Cyrillic characters (for internationalization)
РУССКАЯ_ПЕРЕМЕННАЯ=значение
GREETING="Привет, мир!"

# Edge cases
JUST_EQUALS==
DOUBLE_EQUALS=value==another
STARTS_WITH_QUOTE="value
ENDS_WITH_QUOTE=value"

# Last comment in the file
